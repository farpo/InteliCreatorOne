                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </CheckBox>
                                    <CheckBox fx:id="hasBoots" layoutX="433.0" layoutY="132.0" mnemonicParsing="false" text="Boots" textFill="WHITE">

                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </CheckBox>
                                    <TextField fx:id="armorDurField" layoutX="22.0" layoutY="128.0" prefHeight="25.0" prefWidth="70.0" promptText="Durability" />
